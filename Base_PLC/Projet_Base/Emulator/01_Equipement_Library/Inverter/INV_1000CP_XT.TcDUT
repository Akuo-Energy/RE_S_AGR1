		MesEActDay				: REAL;     (* Daily yield (Wh) [E-heute]*)
		MesWorkingHr			: TIME;     (* Operating time (Hr) [h-On]*)
		MesFeedTm				: TIME;     (* Feed-in Time (Hr) []*)
