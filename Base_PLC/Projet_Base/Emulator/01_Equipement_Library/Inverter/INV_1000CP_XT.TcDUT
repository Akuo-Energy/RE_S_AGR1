STRUCT
		Mesdata					: INV;
		BJMonitor				: INV_1000CP_XT_StringMonitor;	

// Device Family
		MesEventID				: REAL;		(* Event ID of the current event [ErrNo]*)
		MesWaitGridTm			: TIME; 	(* Time until grid connection attempt (s) [TmsRmg].*)
